@model IEnumerable<OpenSchool.Models.CoordinadorCategorias>

@{
    ViewBag.Title = "Index";
}
<div class="col-lg-12">
    <div class="card">
        <div class="card-header d-flex align-items-center">
            <h4>Listado de Coordinadores</h4>
        </div>
        <p>
            @*@Html.ActionLink("Create New", "Create")*@
        </p>
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.CategoriasCursos.DetalleCategoria)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Usuarios.Email)
                </th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.CategoriasCursos.DetalleCategoria)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Usuarios.Email)
                    </td>

                </tr>
            }

        </table>
        </div>
</div>